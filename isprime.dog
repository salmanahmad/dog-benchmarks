include dog

define isPrime: num do
	h = "Prime Number"
	if num%2==0 then
		h = "Not Prime"
	end

	if num%3==0 then
		h = "Not Prime"
	end
	
	a = 5
	b = 2

	while a*a <= num do
		if num%a == 0 then
			h = "Not Prime"
		end

		a = a + 5
		b = 6 - b
	end

	print: h		
end

a = isPrime: 3571

